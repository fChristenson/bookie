{
  // Required
  "manifest_version": 2,
  "name": "Bookie",
  "version": "1.0.0",

  // Recommended
  "description": "A bookmarklet management extension",

  // Pick one (or none)
  "browser_action": {
    "default_icon": "icon.png"
  },

  // Optional
  "author": "Fredrik Christenson",
  "permissions": ["tabs"],
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "css": [],
    "js": ["libs/keypress_listener.js"]
  }
  ]
}